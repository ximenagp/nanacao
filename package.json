  "scripts": {
    "test": "jest --coverage --detectOpenHandles",
    "dev": "node --watch server.js",
    "test:watch": "jest --watch",
    "start": "node server.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "all": "^0.0.0",
    "babel": "^6.23.0",
    "express": "^4.18.1",
    "faker": "^6.6.6",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@babel/generator": "^7.23.6",
    "@babel/preset-env": "^7.23.9",
    "jest": "^29.7.0",
    "supertest": "^6.3.4"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "collectCoverageFrom": [
      "**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
